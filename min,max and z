data <- c(200, 300, 400, 600, 1000)
min_value <- min(data)
max_value <- max(data)
min_max_normalized <- (data - min_value) / (max_value - min_value)
mean_value <- mean(data)
std_dev <- sd(data)
z_score_normalized <- (data - mean_value) / std_dev
cat("Original Data: ", data, "\n")
cat("Min-Max Normalized Data: ", round(min_max_normalized, 4), "\n")
cat("Z-Score Normalized Data: ", round(z_score_normalized, 4), "\n")
